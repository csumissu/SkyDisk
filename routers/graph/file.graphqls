extend type Query {
    listObjects(path: String!): [ObjectResponse]!
}

extend type Mutation {
    singleUpload(path: String!, file: Upload!): Boolean!
    deleteObject(objectID: ID!): Boolean!
}

type ObjectResponse {
    id: ID!
    name: String!
    path: String!
    type: String!
    size: Uint
    mimeType: String
    updatedAt: Time!
    createdAt: Time!
}